i=1;
standing = 0;
total_switches=0;

while (i<length(text(:, 1)))
   if(text(i, 1)==1)
       standing = standing+1;
   else
       if (standing ~= 0)
          fprintf("Standing time: %d\n", standing);
          standing = 0; 
          total_switches=total_switches+1;
       end
   end
    i=i+1;
end
